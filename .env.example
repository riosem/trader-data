# Example environment variables for local development/testing
# Copy this file to .env.local and update with your values
# NEVER commit .env files with real values to version control

# AWS Configuration
export AWS_REGION=us-east-2
export AWS_ACCOUNT_ID=YOUR_ACCOUNT_ID_HERE

# Environment Stage
export STAGE=dev

# Container Configuration
export CONTAINER_IMAGE=YOUR_ACCOUNT_ID.dkr.ecr.us-east-2.amazonaws.com/trader-data-processing:latest
export SECURITY_GROUP_ID=sg-XXXXXXXXXXXXXXXXX
export SUBNET_ID=subnet-XXXXXXXXXXXXXXXXX

# Deployment
export DEPLOYMENT_BUCKET=your-deployment-bucket-name

# Note: Sensitive values like API keys should be stored in AWS SSM Parameter Store
# and referenced through the slsenvs.yml configuration files
